{
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true
  },
  "extends": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
      "react",
      "import",
      "jsx-a11y"
  ],
  "globals": {
      "graphql": true
  },
  "rules": {

  }
}
