    "gatsby-plugin-react-helmet": "^3.x",
    "gatsby-source-filesystem": "^2.x",
    "gatsby-source-mongodb": "^2.x",
    "jss": "^9.x",
    "leaflet": "^1.x",
    "leaflet-heatmap": "^1.x",
    "react": "^16.x",
    "react-dom": "^16.x",
    "react-helmet": "^5.x",
    "react-jss": "^8.x",
    "react-webcam": "^1.x"
  }
