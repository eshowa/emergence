{
  "name": "EmerGence",
  "version": "0.1.0",
  "description": "A Disaster Relief APP",
  "main": "index.js",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "gatsby develop",
    "start": "gatsby serve",
    "clean": "rm -rf public && rm -rf .cache",
    "startDB": "mongod --fork --dbpath ./data --logpath ./logs/mongodb.log",
    "startREST": "python ./db_server/run.py",
    "stopDB": "mongod --shutdown --dbpath ./data &"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikrasov/emergence.git"
  },
  "keywords": [
    "pwa",
    "progressive",
    "web",
    "app"
  ],
  "author": "U Paul",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/u-paul/emergence/issues"
  },
  "homepage": "https://github.com/u-paul/emergence#readme",
  "devDependencies": {
    "eslint": "^5.x",
    "eslint-config-prettier": "^4.x",
    "eslint-plugin-import": "^2.x",
    "eslint-plugin-jsx-a11y": "^6.x",
    "eslint-plugin-react": "^7.x",
    "prettier": "^1.x"
  },
  "dependencies": {
    "@material-ui/core": "^3.x",
    "@material-ui/icons": "^3.x",
    "classnames": "^2.x",
    "core-js": "^2.6.4",
    "gatsby": "^2.x",
    "gatsby-image": "^2.x",
    "gatsby-plugin-catch-links": "^2.x",
    "gatsby-plugin-manifest": "^2.x",
